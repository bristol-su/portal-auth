{
  "name": "bristol-su/portal-auth",
  "description": "Authentication for Portal SDK based software",
  "license": "GPL-3.0-or-later",
  "prefer-stable": true,
  "keywords": [
    "portal",
    "sdk",
    "bristol su",
    "auth",
    "login"
  ],
  "authors": [
    {
      "name": "Toby Twigger",
      "email": "toby.twigger@bristol.ac.uk"
    }
  ],
  "type": "library",
  "require": {
    "php": "^7.4",
    "laravel/framework": "^8.0",
    "laravel/passport": "^10.0",
    "bristol-su/control": "dev-laravel-8",
    "doctrine/dbal": "^2.10"

  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpstan/phpstan": "^0.12.3",
    "brianium/paratest": "^4.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "nunomaduro/phpinsights": "^1.14"
  },
  "autoload": {
    "files": [
      "src/helpers/helpers.php"
    ],
    "psr-4": {
      "BristolSU\\Auth\\": "src/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BristolSU\\Support\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BristolSU\\Auth\\AuthServiceProvider"
      ]
    }
  }
}
